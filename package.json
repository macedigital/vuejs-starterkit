    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.10.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
