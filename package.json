{
  "name": "vuejs-starterkit",
  "version": "1.0.0",
  "description": "A Vue.js starter-kit project",
  "keywords": [
    "vue",
    "vuex",
    "vue-loader",
    "vue-router",
    "webpack"
  ],
  "author": "Matthias Adler",
  "private": true,
